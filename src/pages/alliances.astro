---
import BaseLayout from '../layouts/BaseLayout.astro';

// Partnership benefits
const partnershipBenefits = [
  {
    number: '01',
    title: '70+ Years Combined Experience',
    description: 'Benefits emanating from over 70 years of combined reinsurance broking experience through our strategic partnerships, spanning multiple market cycles and regulatory environments.'
  },
  {
    number: '02',
    title: 'Global Market Access',
    description: 'Access to global reinsurance markets through our network in leading global centres, enabling competitive terms and diversified capacity for complex risks.'
  },
  {
    number: '03',
    title: 'Cutting-Edge Analytical Tools',
    description: 'Access to advanced reinsurance modelling tools enabling sophisticated catastrophe modelling, portfolio optimisation, and data-driven decision support.'
  },
  {
    number: '04',
    title: 'Specialist Technical Resources',
    description: 'Access to specialist resources including qualified Actuaries, Life & Health experts, Risk Engineers, and Agricultural specialists.'
  },
  {
    number: '05',
    title: 'Integrated Business Intelligence',
    description: 'Robust and integrated broking systems with business intelligence tools to produce meaningful data for reinsurance optimisation and strategic planning.'
  },
];

// Technical resources
const technicalResources = [
  {
    title: 'Actuarial Services',
    description: 'Qualified actuaries provide analytical support for pricing, reserving, and portfolio analysis.',
    icon: 'calculator'
  },
  {
    title: 'Life & Health Expertise',
    description: 'Specialists in mortality, morbidity, and long-term risk assessment support our life and health practice.',
    icon: 'heart'
  },
  {
    title: 'Engineering Services',
    description: 'Risk engineers with technical backgrounds provide loss prevention advice and risk assessment.',
    icon: 'cog'
  },
  {
    title: 'Agronomy Services',
    description: 'Agricultural specialists support crop insurance and agribusiness placements with technical input.',
    icon: 'leaf'
  },
];

// Partnership principles
const principles = [
  { title: 'Alignment of Interests', description: 'We partner with organisations that share our commitment to client service and professional integrity.' },
  { title: 'Complementary Capability', description: 'Partnerships are structured to enhance our service offering without creating conflicts or dependencies.' },
  { title: 'Mutual Benefit', description: 'Successful partnerships create value for all parties, including most importantly our clients.' },
  { title: 'Long-term Orientation', description: 'We invest in relationships built for sustained collaboration rather than transactional convenience.' },
];

// Client impact items
const clientImpacts = [
  'Access to broader market capacity and competitive terms',
  'Technical support that enhances programme design and risk selection',
  'Analytical tools that improve decision-making quality',
  'Operational efficiency through integrated systems and processes',
  'Specialist expertise for complex or technical placements',
];
---

<BaseLayout title="Strategic Partnerships | Guardian Reinsurance Brokers">
  <!-- Hero Section -->
  <section class="relative bg-gradient-to-br from-guardian-green via-guardian-green-dark to-guardian-black py-24 md:py-32 overflow-hidden">
    <!-- Animated decorative sun rays -->
    <div class="absolute top-0 right-0 w-[500px] h-[500px] opacity-10">
      <svg viewBox="0 0 200 200" class="w-full h-full animate-spin-very-slow">
        {[...Array(16)].map((_, i) => (
          <line
            x1="100"
            y1="100"
            x2={100 + 100 * Math.cos((i * 22.5) * Math.PI / 180)}
            y2={100 + 100 * Math.sin((i * 22.5) * Math.PI / 180)}
            stroke="#CBA875"
            stroke-width="2"
          />
        ))}
        <circle cx="100" cy="100" r="30" fill="#CBA875" opacity="0.3"/>
      </svg>
    </div>

    <!-- Floating dots decoration -->
    <div class="absolute inset-0 overflow-hidden pointer-events-none">
      <div class="absolute top-20 left-[10%] w-2 h-2 bg-guardian-gold/30 rounded-full animate-float"></div>
      <div class="absolute top-40 left-[20%] w-3 h-3 bg-guardian-gold/20 rounded-full animate-float" style="animation-delay: 1s;"></div>
      <div class="absolute bottom-32 left-[15%] w-2 h-2 bg-guardian-gold/25 rounded-full animate-float" style="animation-delay: 2s;"></div>
      <div class="absolute top-32 right-[25%] w-2 h-2 bg-guardian-gold/30 rounded-full animate-float" style="animation-delay: 0.5s;"></div>
      <div class="absolute bottom-40 right-[10%] w-3 h-3 bg-guardian-gold/20 rounded-full animate-float" style="animation-delay: 1.5s;"></div>
    </div>

    <div class="container relative z-10">
      <div class="max-w-3xl mx-auto text-center">
        <div class="animate-fade-in-up inline-flex items-center gap-2 px-5 py-2.5 bg-white/10 rounded-full text-sm text-white mb-8 backdrop-blur-sm border border-white/10">
          <span class="w-2 h-2 bg-guardian-gold rounded-full animate-pulse"></span>
          Global Reach, Local Expertise
        </div>

        <h1 class="animate-fade-in-up text-4xl md:text-5xl lg:text-6xl font-serif font-bold text-white mb-6" style="animation-delay: 0.1s;">
          Strategic
          <span class="block section-gold-text mt-2">Partnerships</span>
        </h1>

        <p class="animate-fade-in-up text-xl text-white/90 leading-relaxed max-w-2xl mx-auto" style="animation-delay: 0.2s;">
          Extending our capabilities and enhancing the value we deliver through relationships built on shared professional values and mutual commitment to excellence.
        </p>
      </div>
    </div>

  </section>

  <!-- Partnership Value Intro - Floating Card -->
  <section class="relative -mt-16 z-20 px-4">
    <div class="container">
      <div class="animate-scale-in max-w-4xl mx-auto bg-white rounded-3xl shadow-2xl border border-gray-100 p-8 md:p-12 overflow-hidden relative">
        <div class="absolute top-0 left-0 right-0 h-1 bg-gradient-to-r from-guardian-green via-guardian-gold to-guardian-green"></div>

        <div class="text-center">
          <p class="text-lg text-gray-600 leading-relaxed mb-6">
            Guardian Re has established strategic partnerships that extend our capabilities and enhance the value we deliver to clients. These relationships are built on shared professional values, complementary expertise, and a mutual commitment to service excellence.
          </p>
          <p class="text-gray-600 leading-relaxed mb-8">
            Our partnership approach enables us to offer clients access to global reinsurance markets, specialist technical resources, and advanced analytical tools that would otherwise be beyond the reach of a regional broker.
          </p>

          <!-- Experience Callout -->
          <div class="inline-flex items-center gap-4 px-8 py-4 bg-gradient-to-r from-guardian-gold/10 to-guardian-gold/5 rounded-2xl border border-guardian-gold/20">
            <div class="w-12 h-12 bg-gradient-to-br from-guardian-gold to-guardian-gold-light rounded-xl flex items-center justify-center shadow-lg">
              <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/>
              </svg>
            </div>
            <div class="text-left">
              <p class="text-2xl font-bold text-gray-900">70+</p>
              <p class="text-sm text-gray-600">Years Combined Experience</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Partnership Benefits Grid -->
  <section class="py-20 md:py-28 bg-gray-50">
    <div class="container">
      <div class="text-center mb-16">
        <span class="animate-fade-in-up inline-block px-5 py-2 bg-guardian-green/10 text-guardian-green text-sm font-semibold rounded-full mb-4">Partnership Value</span>
        <h2 class="animate-fade-in-up text-3xl md:text-4xl lg:text-5xl font-serif font-bold text-gray-900 mb-4" style="animation-delay: 0.1s;">
          The Benefits <span class="section-gold-text">We Bring</span>
        </h2>
        <p class="animate-fade-in-up text-lg text-gray-600 max-w-2xl mx-auto" style="animation-delay: 0.2s;">
          Our partnerships translate into tangible benefits for the insurers we serve.
        </p>
      </div>

      <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6 md:gap-8 max-w-6xl mx-auto">
        {partnershipBenefits.map((benefit, index) => (
          <div
            class="animate-fade-in-up group bg-white rounded-2xl overflow-hidden shadow-lg hover:shadow-2xl transition-all duration-500 hover:-translate-y-2"
            style={`animation-delay: ${0.1 * index}s;`}
          >
            <!-- Gold accent line on hover -->
            <div class="h-1 bg-gradient-to-r from-guardian-gold via-guardian-gold-light to-guardian-gold transform scale-x-0 group-hover:scale-x-100 transition-transform duration-500 origin-left"></div>

            <div class="p-6 md:p-8">
              <div class="w-12 h-12 bg-gradient-to-br from-guardian-gold/20 to-guardian-gold/5 rounded-xl flex items-center justify-center mb-5 group-hover:scale-110 transition-transform duration-300">
                <span class="text-guardian-gold font-bold text-lg">{benefit.number}</span>
              </div>
              <h3 class="text-xl font-semibold text-gray-900 mb-3 group-hover:text-guardian-gold transition-colors">{benefit.title}</h3>
              <p class="text-gray-600 leading-relaxed">{benefit.description}</p>
            </div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Technical Resources -->
  <section class="py-20 md:py-28 bg-white">
    <div class="container">
      <div class="text-center mb-16">
        <span class="animate-fade-in-up inline-block px-5 py-2 bg-guardian-gold/10 text-guardian-gold text-sm font-semibold rounded-full mb-4">Specialist Capabilities</span>
        <h2 class="animate-fade-in-up text-3xl md:text-4xl lg:text-5xl font-serif font-bold text-gray-900 mb-4" style="animation-delay: 0.1s;">
          Technical <span class="section-gold-text">Resources</span>
        </h2>
        <p class="animate-fade-in-up text-lg text-gray-600 max-w-2xl mx-auto" style="animation-delay: 0.2s;">
          Our partnerships provide access to specialist resources that support comprehensive client service.
        </p>
      </div>

      <div class="grid sm:grid-cols-2 lg:grid-cols-4 gap-6 md:gap-8 max-w-5xl mx-auto">
        {technicalResources.map((resource, index) => (
          <div
            class="animate-fade-in-up group text-center p-8 bg-gray-50 rounded-2xl hover:bg-white hover:shadow-xl transition-all duration-500 hover:-translate-y-2"
            style={`animation-delay: ${0.1 * index}s;`}
          >
            <div class="w-16 h-16 mx-auto mb-5 bg-gradient-to-br from-guardian-green/10 to-guardian-green/5 rounded-2xl flex items-center justify-center group-hover:scale-110 transition-transform duration-300">
              {resource.icon === 'calculator' && (
                <svg class="w-8 h-8 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 7h6m0 10v-3m-3 3h.01M9 17h.01M9 14h.01M12 14h.01M15 11h.01M12 11h.01M9 11h.01M7 21h10a2 2 0 002-2V5a2 2 0 00-2-2H7a2 2 0 00-2 2v14a2 2 0 002 2z"/>
                </svg>
              )}
              {resource.icon === 'heart' && (
                <svg class="w-8 h-8 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z"/>
                </svg>
              )}
              {resource.icon === 'cog' && (
                <svg class="w-8 h-8 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"/>
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"/>
                </svg>
              )}
              {resource.icon === 'leaf' && (
                <svg class="w-8 h-8 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 3v4M3 5h4M6 17v4m-2-2h4m5-16l2.286 6.857L21 12l-5.714 2.143L13 21l-2.286-6.857L5 12l5.714-2.143L13 3z"/>
                </svg>
              )}
            </div>
            <h3 class="text-lg font-semibold text-gray-900 mb-3 group-hover:text-guardian-green transition-colors">{resource.title}</h3>
            <p class="text-sm text-gray-600 leading-relaxed">{resource.description}</p>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Minerva Re Partnership Feature -->
  <section class="py-20 md:py-28 bg-gradient-to-br from-guardian-cream to-white">
    <div class="container">
      <div class="grid lg:grid-cols-5 gap-10 lg:gap-16 items-center max-w-6xl mx-auto">
        <!-- Logo Area -->
        <div class="lg:col-span-2 flex flex-col items-center animate-slide-in-left">
          <div class="bg-white rounded-3xl p-8 md:p-10 shadow-2xl border border-guardian-gold/10 relative overflow-hidden">
            <!-- Decorative corner -->
            <div class="absolute top-0 right-0 w-24 h-24 bg-gradient-to-br from-guardian-gold/10 to-transparent"></div>
            <img
              src="/images/guardian-logo.jpg"
              alt="Guardian Reinsurance Brokers - In Association with Minerva Re"
              class="h-28 md:h-36 w-auto object-contain relative z-10"
            />
          </div>
          <div class="mt-6 px-5 py-2 bg-guardian-gold/10 rounded-full">
            <p class="text-sm font-semibold text-guardian-gold">Endorsed Broking Partner</p>
          </div>
        </div>

        <!-- Content Area -->
        <div class="lg:col-span-3 animate-slide-in-right">
          <span class="inline-block px-5 py-2 bg-guardian-green/10 text-guardian-green text-sm font-semibold rounded-full mb-6">Primary Partnership</span>
          <h2 class="text-3xl md:text-4xl lg:text-5xl font-serif font-bold text-gray-900 mb-6">
            <a href="https://www.minerva.co.zw/" target="_blank" rel="noopener noreferrer" class="hover:text-guardian-gold transition-colors">Minerva Re</a> <span class="section-gold-text">Partnership</span>
          </h2>
          <p class="text-lg text-gray-600 mb-6 leading-relaxed">
            Guardian Re is proud to serve as an endorsed reinsurance broking partner of <a href="https://www.minerva.co.zw/" target="_blank" rel="noopener noreferrer" class="text-guardian-gold font-semibold hover:underline">Minerva Re</a>. This relationship provides our clients with access to Minerva Re's global network, technical expertise, and market relationships.
          </p>
          <p class="text-gray-600 mb-8 leading-relaxed">
            The partnership enhances our ability to serve clients with complex requirements, large capacity needs, or specialist risk transfer challenges.
          </p>

          <!-- Benefits List -->
          <div class="grid sm:grid-cols-2 gap-4">
            <div class="flex items-start gap-4 p-4 bg-white rounded-xl shadow-sm hover:shadow-md transition-shadow">
              <div class="w-8 h-8 bg-guardian-green/10 rounded-lg flex items-center justify-center flex-shrink-0">
                <svg class="w-4 h-4 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                </svg>
              </div>
              <span class="text-gray-700">Access to global reinsurance network</span>
            </div>
            <div class="flex items-start gap-4 p-4 bg-white rounded-xl shadow-sm hover:shadow-md transition-shadow">
              <div class="w-8 h-8 bg-guardian-green/10 rounded-lg flex items-center justify-center flex-shrink-0">
                <svg class="w-4 h-4 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                </svg>
              </div>
              <span class="text-gray-700">Technical expertise and market intelligence</span>
            </div>
            <div class="flex items-start gap-4 p-4 bg-white rounded-xl shadow-sm hover:shadow-md transition-shadow">
              <div class="w-8 h-8 bg-guardian-green/10 rounded-lg flex items-center justify-center flex-shrink-0">
                <svg class="w-4 h-4 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                </svg>
              </div>
              <span class="text-gray-700">Enhanced capacity for complex placements</span>
            </div>
            <div class="flex items-start gap-4 p-4 bg-white rounded-xl shadow-sm hover:shadow-md transition-shadow">
              <div class="w-8 h-8 bg-guardian-green/10 rounded-lg flex items-center justify-center flex-shrink-0">
                <svg class="w-4 h-4 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                </svg>
              </div>
              <span class="text-gray-700">Shared commitment to service excellence</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Advanced Analytics & Systems -->
  <section class="py-20 md:py-28 bg-white">
    <div class="container">
      <div class="grid md:grid-cols-2 gap-8 max-w-5xl mx-auto">
        <!-- Analytics Card -->
        <div class="animate-slide-in-left group bg-gray-50 rounded-3xl p-8 md:p-10 hover:bg-white hover:shadow-2xl transition-all duration-500 border border-transparent hover:border-gray-100">
          <div class="flex items-center gap-4 mb-6">
            <div class="w-14 h-14 bg-gradient-to-br from-guardian-green/10 to-guardian-green/5 rounded-2xl flex items-center justify-center group-hover:scale-110 transition-transform duration-300">
              <svg class="w-7 h-7 text-guardian-green" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
              </svg>
            </div>
            <h3 class="text-2xl font-semibold text-gray-900">Advanced Analytics</h3>
          </div>
          <p class="text-gray-600 mb-6 leading-relaxed">
            Our partners provide access to advanced reinsurance modelling tools and business intelligence platforms.
          </p>
          <ul class="space-y-3">
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Sophisticated catastrophe modelling
            </li>
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Portfolio optimisation and scenario testing
            </li>
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Loss projection and pricing validation
            </li>
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Data-driven decision support
            </li>
          </ul>
        </div>

        <!-- Systems Card -->
        <div class="animate-slide-in-right group bg-gray-50 rounded-3xl p-8 md:p-10 hover:bg-white hover:shadow-2xl transition-all duration-500 border border-transparent hover:border-gray-100" style="animation-delay: 0.15s;">
          <div class="flex items-center gap-4 mb-6">
            <div class="w-14 h-14 bg-gradient-to-br from-guardian-gold/10 to-guardian-gold/5 rounded-2xl flex items-center justify-center group-hover:scale-110 transition-transform duration-300">
              <svg class="w-7 h-7 text-guardian-gold" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 7v10c0 2.21 3.582 4 8 4s8-1.79 8-4V7M4 7c0 2.21 3.582 4 8 4s8-1.79 8-4M4 7c0-2.21 3.582-4 8-4s8 1.79 8 4m0 5c0 2.21-3.582 4-8 4s-8-1.79-8-4"/>
              </svg>
            </div>
            <h3 class="text-2xl font-semibold text-gray-900">Integrated Systems</h3>
          </div>
          <p class="text-gray-600 mb-6 leading-relaxed">
            Guardian Re utilises an integrated broking platform with advanced business intelligence capabilities.
          </p>
          <ul class="space-y-3">
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Efficient programme administration
            </li>
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Accurate and timely reporting
            </li>
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Comprehensive data management
            </li>
            <li class="flex items-center gap-3 text-gray-600">
              <span class="w-2 h-2 bg-guardian-gold rounded-full"></span>
              Meaningful management information
            </li>
          </ul>
        </div>
      </div>
    </div>
  </section>

  <!-- Partnership Principles -->
  <section class="py-20 md:py-28 bg-guardian-green text-white relative overflow-hidden">
    <!-- Background pattern -->
    <div class="absolute inset-0 opacity-10">
      <svg class="w-full h-full" viewBox="0 0 100 100" preserveAspectRatio="none">
        <pattern id="principles-grid" width="20" height="20" patternUnits="userSpaceOnUse">
          <circle cx="10" cy="10" r="1" fill="#CBA875"/>
        </pattern>
        <rect width="100%" height="100%" fill="url(#principles-grid)"/>
      </svg>
    </div>

    <!-- Floating decorative circles -->
    <div class="absolute top-10 left-10 w-32 h-32 border border-guardian-gold/20 rounded-full animate-float"></div>
    <div class="absolute bottom-10 right-10 w-24 h-24 border border-guardian-gold/20 rounded-full animate-float" style="animation-delay: 1s;"></div>

    <div class="container relative z-10">
      <div class="text-center mb-16">
        <span class="animate-fade-in-up inline-block px-5 py-2 bg-white/10 text-guardian-gold text-sm font-semibold rounded-full mb-6 backdrop-blur-sm border border-white/10">Our Approach</span>
        <h2 class="animate-fade-in-up text-3xl md:text-4xl lg:text-5xl font-serif font-bold mb-6" style="animation-delay: 0.1s;">
          Partnership <span class="section-gold-text">Principles</span>
        </h2>
        <p class="animate-fade-in-up text-xl text-white/90 max-w-2xl mx-auto" style="animation-delay: 0.2s;">
          Our approach to partnerships is guided by clear principles.
        </p>
      </div>

      <div class="grid sm:grid-cols-2 lg:grid-cols-4 gap-6 md:gap-8 max-w-6xl mx-auto">
        {principles.map((principle, index) => (
          <div
            class="animate-fade-in-up group bg-white/5 backdrop-blur-sm rounded-2xl p-8 border border-white/10 hover:bg-white/10 transition-all duration-500 hover:-translate-y-2"
            style={`animation-delay: ${0.1 * index}s;`}
          >
